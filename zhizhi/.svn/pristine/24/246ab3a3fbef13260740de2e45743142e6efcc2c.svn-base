.u-loginfailtip{margin-left: 135px;}
.u-submitbtn{width: 300px;margin-left: 35px;margin-top:5px;background-color: #4aa1ec;}
.u-forgetpwdtip{font-weight: bold;position:relative; left:140px;top:15px; font-size: 14px;}
.u-luokuan{margin-top:50px; float: right;margin-right: 10px;}